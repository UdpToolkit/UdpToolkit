{
  "metadata": [
    {
      "src": [
        {
          "src": "../src/shared/",
          "dest": "api",
          "files": [
            "/**/*.dll"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "documentation/**.md",
          "documentation/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "UdpToolkit website",
      "_gitContribute": {
        "repo": "https://github.com/UdpToolkit/UdpToolkit",
        "branch": "master",
        "apiSpecFolder": "docs-ref-overwrite"
      }
    },
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}